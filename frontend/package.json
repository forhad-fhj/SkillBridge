{
  "name": "skillbridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "next dev --turbopack --experimental-debug",
    "build": "next build",
    "start": "next start",
